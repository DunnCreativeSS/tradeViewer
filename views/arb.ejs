<table>
<tr>
     <td>time</td>
     <td>ticker</td>
     <td>bid</td>
     <td>ask</td>
     <td>arb %</td>
     <td>bid exchange</td>
     <td>ask exchange</td>
     <td>threshold (base currency)</td>
